<div class="row">
    <% rentalsystems.forEach( function(model) { %>
        <div class="card col-md-6 col-sm-12" >
            <a href="http://localhost:1337/rentalsystem/detail/<%=model.id%>" class="btn btn-link">
                <%=model.Propertytitle%>
            </a>
            <dic class="row">
                <div class="col-md-4">
                    <img src="<%=model.image%>" class="card-img" width="150px" height="150px">
                </div>
                <div class="col-md-8">
                    <div class="card">
                        <ul class="list-group list-group-flush">
                            <li class="list-group-item">
                                <b>Estate:</b>
                                <%=model.Estate%>
                            </li>
                            <li class="list-group-item">
                                <b>Gross Area:</b>
                                <%=model.area%>
                            </li>
                            <li class="list-group-item">
                                <b>Rent: </b>
                                <%=model.rent%>
                            </li>
                        </ul>
                    </div>

                </div>
            </dic>


        </div>
        <% }); %>
</div>